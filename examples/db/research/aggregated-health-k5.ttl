@prefix : <https://openprovenance.org/ns/facet/sdc#> .
@prefix dsf: <https://openprovenance.org/ns/facet/dsf#> .
@prefix base: <https://openprovenance.org/ns/facet/base#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix app: <https://example.org/medical#> .

# Aggregated health data with k-anonymity = 5 (common HIPAA practice)
# Demonstrates framework divergence at medium k values
app:AggregatedHealth5 a :Table ;
  dsf:hasTag base:Healthcare ;
  dsf:hasTag base:Individual ;
  :declaresField [
    :forFieldName "recordId" ;
    :hasLabel base:InternalUUID  # Meaningless internal identifier
  ] ;
  :declaresField [
    :forFieldName "maritalStatus" ;
    :hasLabel base:MaritalStatus
  ] ;
  :declaresField [
    :forFieldName "postalCode" ;
    :hasLabel base:ZipCode
  ] ;
  :declaresField [
    :forFieldName "year" ;
    :hasLabel base:Age
  ] ;
  :declaresField [
    :forFieldName "diagCode" ;
    :hasLabel base:DiagnosisCode
  ] ;
  :hasRecords
    # Cohort: Single, 94105, 1975, I10 (5 records for k=5)
    [ a :Record ;
      :hasField [ :fieldName "recordId"; :fieldValue "k5-001" ],
                [ :fieldName "maritalStatus"; :fieldValue "Single" ],
                [ :fieldName "postalCode"; :fieldValue "94105" ],
                [ :fieldName "year"; :fieldValue "1975"^^xsd:integer ],
                [ :fieldName "diagCode"; :fieldValue "I10" ] ],
    [ a :Record ;
      :hasField [ :fieldName "recordId"; :fieldValue "k5-002" ],
                [ :fieldName "maritalStatus"; :fieldValue "Single" ],
                [ :fieldName "postalCode"; :fieldValue "94105" ],
                [ :fieldName "year"; :fieldValue "1975"^^xsd:integer ],
                [ :fieldName "diagCode"; :fieldValue "I10" ] ],
    [ a :Record ;
      :hasField [ :fieldName "recordId"; :fieldValue "k5-003" ],
                [ :fieldName "maritalStatus"; :fieldValue "Single" ],
                [ :fieldName "postalCode"; :fieldValue "94105" ],
                [ :fieldName "year"; :fieldValue "1975"^^xsd:integer ],
                [ :fieldName "diagCode"; :fieldValue "I10" ] ],
    [ a :Record ;
      :hasField [ :fieldName "recordId"; :fieldValue "k5-004" ],
                [ :fieldName "maritalStatus"; :fieldValue "Single" ],
                [ :fieldName "postalCode"; :fieldValue "94105" ],
                [ :fieldName "year"; :fieldValue "1975"^^xsd:integer ],
                [ :fieldName "diagCode"; :fieldValue "I10" ] ],
    [ a :Record ;
      :hasField [ :fieldName "recordId"; :fieldValue "k5-005" ],
                [ :fieldName "maritalStatus"; :fieldValue "Single" ],
                [ :fieldName "postalCode"; :fieldValue "94105" ],
                [ :fieldName "year"; :fieldValue "1975"^^xsd:integer ],
                [ :fieldName "diagCode"; :fieldValue "I10" ] ] .